{
	"name": "@polar-sh/ingestion",
	"version": "0.0.6",
	"description": "Core package for Polar ingestion",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"import": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"require": "./dist/index.cjs"
			}
		},
		"./strategies/StreamStrategy": {
			"import": {
				"types": "./dist/strategies/StreamStrategy/StreamStrategy.d.ts",
				"import": "./dist/strategies/StreamStrategy/StreamStrategy.js"
			},
			"require": {
				"types": "./dist/strategies/StreamStrategy/StreamStrategy.d.cts",
				"require": "./dist/strategies/StreamStrategy/StreamStrategy.cjs"
			}
		},
		"./strategies/LLMStrategy": {
			"import": {
				"types": "./dist/strategies/LLMStrategy/LLMStrategy.d.ts",
				"import": "./dist/strategies/LLMStrategy/LLMStrategy.js"
			},
			"require": {
				"types": "./dist/strategies/LLMStrategy/LLMStrategy.d.cts",
				"require": "./dist/strategies/LLMStrategy/LLMStrategy.cjs"
			}
		},
		"./strategies/S3Strategy": {
			"import": {
				"types": "./dist/strategies/S3Strategy/S3Strategy.d.ts",
				"import": "./dist/strategies/S3Strategy/S3Strategy.js"
			},
			"require": {
				"types": "./dist/strategies/S3Strategy/S3Strategy.d.cts",
				"require": "./dist/strategies/S3Strategy/S3Strategy.cjs"
			}
		}
	},
	"scripts": {
		"build": "tsup",
		"test": "vitest"
	},
	"keywords": [],
	"author": "",
	"license": "Apache-2.0",
	"dependencies": {
		"@polar-sh/sdk": "^0.24.0"
	},
	"peerDependencies": {
		"@aws-sdk/client-s3": "^3.744.0",
		"ai": "^4.1.17"
	},
	"devDependencies": {
		"@ai-sdk/openai": "^1.1.9",
		"@ai-sdk/provider": "^1.0.7",
		"@aws-sdk/client-s3": "^3.744.0",
		"@sindresorhus/tsconfig": "^7.0.0",
		"ai": "^4.1.17",
		"tsup": "^8.3.5",
		"typescript": "^5.7.3",
		"vitest": "^2.1.8"
	}
}
